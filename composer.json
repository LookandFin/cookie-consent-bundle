{
    "name": "connectholland/cookie-consent-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for implementing Cookie Consent to comply to AVG/GDPR.",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "doctrine/annotations": "^1.6",
        "doctrine/doctrine-bundle": "^1.10 || ^2.0",
        "doctrine/orm": "^2.5",
        "symfony/asset": "^6.0 || ^7.1",
        "symfony/dependency-injection": "^6.0 || ^7.1",
        "symfony/doctrine-bridge": "^6.0 || ^7.1",
        "symfony/expression-language": "^6.0 || ^7.1",
        "symfony/form": "^6.0 || ^7.1",
        "symfony/framework-bundle": "^6.0 || ^7.1",
        "symfony/http-foundation": "^6.0 || ^7.1",
        "symfony/templating": "^6.0 || ^7.1",
        "symfony/translation": "^6.0 || ^7.1",
        "symfony/twig-bridge": "^6.0 || ^7.1",
        "symfony/twig-bundle": "^6.0 || ^7.1",
        "twig/twig": "^2.6 || ^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpunit/phpunit": "^7.5",
        "symfony/yaml": "^3.4 || ^4.2 || ^5.0 || ^6.0"
    },
    "prefer-stable": true,
    "autoload": {
        "exclude-from-classmap": ["/Tests/"],
        "psr-4": {
            "ConnectHolland\\CookieConsentBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ConnectHolland\\CookieConsentBundle\\Tests\\": "/Tests"
        }
    },
    "config": {
         "sort-packages": true
    }
}
